		(
			EMPATHY_TOPSTORY,
			EMPATHY_THEME1,
			EMPATHY_THEME2
		)
	VALUES(
			#{empathy_topstory},
			#{empathy_theme1},
			#{empathy_theme2}
		)
</insert>
